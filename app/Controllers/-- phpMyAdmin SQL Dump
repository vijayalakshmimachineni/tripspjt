							year_id,
							admission_number,
							student_id,
							roll_number,
							course_id,
							daycare_id,
							status,
							created_by,
							created_date
							)
			VALUES
							(p_branch_id,
							p_year_id,
							p_admission_number,
							p_student_id,
							p_roll_number,
							p_course_id,
							p_daycare_id,
							p_status,
							p_created_by,
							p_created_date);


			IF row_count() = 0 THEN

				SET p_result = "Sorry!Please try once again";
			ELSE
				SET p_result = "Sucess";
			END IF;  


END$$

