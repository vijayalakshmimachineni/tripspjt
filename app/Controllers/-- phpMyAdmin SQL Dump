p_route_name,
p_start_point,
p_end_point,
p_threshold,
p_status,
p_created_by,
p_created_date);
					

IF row_count() = 0 THEN
               
				SET p_result = "Sorry!Please try once again";
            ELSE
                       SET p_result = "Sucess";
				
             END IF;

END$$

