BEGIN

DECLARE v_fee_name  VARCHAR(250);

							SELECT 
                                             				 ( SELECT 
                                                              						print_name
                                                              	FROM
                                                                                   tbl_fee_type f             
                                                              	WHERE
                                                              						f.fee_type_id = tp.fee_type_id
                                                              	LIMIT 0, 1 )  as  fee_type_name
							INTO 
												v_fee_name
							FROM  
												`tbl_fee_type_plan` tp 

							WHERE
													type_plan_id =  p_type_plan_id 
                             LIMIT 0,1 ;



RETURN  v_fee_name;

END$$

