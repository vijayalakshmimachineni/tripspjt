SELECT  
		course_name
INTO	
		v_course_name
FROM	
		tbl_course
WHERE	
		course_id = (SELECT cource_id	FROM tbl_fee_cource_plan	WHERE cource_plan_id = p_cource_plan_id LIMIT 0,1);

RETURN v_course_name;


END$$

